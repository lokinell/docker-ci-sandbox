    </jdk>
  </jdks>
  <slaves>
    <slave>
      <name>slave1</name>
      <description></description>
      <remoteFS>/var/lib/jenkins-slave</remoteFS>
      <numExecutors>2</numExecutors>
      <mode>NORMAL</mode>
      <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
      <launcher class="hudson.plugins.sshslaves.SSHLauncher" plugin="ssh-slaves@1.5">
        <host>jenkinsSlave_1</host>
        <port>22</port>
        <credentialsId>a055e4f5-43b7-4d5f-a222-1c1d703fa730</credentialsId>
      </launcher>
      <label>linux linux-x64 java</label>
      <nodeProperties/>
    </slave>
  </slaves>
  <globalNodeProperties>
    <hudson.slaves.EnvironmentVariablesNodeProperty>
      <envVars serialization="custom">
        <unserializable-parents/>
        <tree-map>
          <default>
            <comparator class="hudson.util.CaseInsensitiveComparator"/>
          </default>
          <int>2</int>
          <string>GIT_SERVER_URL</string>
          <string>ssh://jenkins@gerrit:29418</string>
          <string>NEXUS_URL</string>
          <string>http://nexus:8081/nexus</string>
        </tree-map>
      </envVars>
    </hudson.slaves.EnvironmentVariablesNodeProperty>
  </globalNodeProperties>
  <noUsageStatistics>true</noUsageStatistics>
