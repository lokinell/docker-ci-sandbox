  <securityRealm class="hudson.security.LDAPSecurityRealm">
    <server>ldap</server>
    <rootDN>dc=asf,dc=griddynamics,dc=com</rootDN>
    <userSearchBase>ou=people</userSearchBase>
    <userSearch>uid={0}</userSearch>
    <groupSearchBase>ou=groups</groupSearchBase>
    <managerDN>cn=admin,dc=asf,dc=griddynamics,dc=com</managerDN>
    <managerPassword>YWRtaW4=</managerPassword>     <!-- FIXME - hardcoded crypted password -->
    <disableMailAddressResolver>false</disableMailAddressResolver>
  </securityRealm>
  <workspaceDir>/var/lib/jenkins-ws/${ITEM_FULLNAME}</workspaceDir>
  <buildsDir>/var/lib/jenkins-builds/${ITEM_FULLNAME}</buildsDir>
  <jdks>
    <jdk>
      <name>jdk-default</name>
      <home></home>
      <properties>
        <hudson.tools.InstallSourceProperty>
          <installers>
            <hudson.tools.ZipExtractionInstaller>
              <label>java &amp;&amp; linux-x64</label>
              <url>http://nexus:8081/nexus/content/repositories/thirdparty/com/oracle/java/jdk/7u67/jdk-7u67-linux-x64.tar.gz</url>
              <subdir>jdk1.7.0_67</subdir>
            </hudson.tools.ZipExtractionInstaller>
          </installers>
        </hudson.tools.InstallSourceProperty>
      </properties>
    </jdk>
    <jdk>
      <name>jdk-7</name>
      <home></home>
      <properties>
        <hudson.tools.InstallSourceProperty>
          <installers>
            <hudson.tools.ZipExtractionInstaller>
              <label>java &amp;&amp; linux-x64</label>
              <url>http://nexus:8081/nexus/content/repositories/thirdparty/com/oracle/java/jdk/7u67/jdk-7u67-linux-x64.tar.gz</url>
